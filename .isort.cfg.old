[settings]
py_version=39
atomic=true
force_alphabetical_sort=false
multi_line_output=3
line_length=88
include_trailing_comma=true
split_on_trailing_comma=true
use_parentheses=true
ensure_newline_before_comments=true
lines_after_imports=2
force_grid_wrap=0
sections=FUTURE,STDLIB,THIRDPARTY,FIRSTPARTY,LOCALFOLDER
skip=venv/
skip_glob=**/migrations/*.py
known_first_party=minutes,mailing,membership,peps,boxes,successstories,jobs,custom_storages,work_groups,sponsors,codesamples,companies,blogs,cms,downloads,nominations,users,pydotorg,community,pages,banners,events,fastly
